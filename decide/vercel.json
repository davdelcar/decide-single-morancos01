{
    "builds": [
        {
            "src": "decide/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": { "distDir": "decide/staticfiles_build" }
        }
    ],
    "routes": [

        {
            
        },
        
        { "src": "/(.*)", "dest": "decide/wsgi.py" }
    ]
}